package ncs.test5;

public class Book {
//필드 
	private String title;
	private String author;
	private int price;
	private String publisher;
	double discountRet;
	
	//기본생성자 
	public Book() {
		 
	}

	//필수생성자
	
	//Getter
	//Setter
	public String getTitle() {
		return title;
	}
	public String getAuthor() {
		return author;
	}
	public int getPrice() {
		return price;
	}
	public String getPublisher() {
		return publisher;
	}
	public double getDiscountRet() {
		return discountRet;
	}
}
